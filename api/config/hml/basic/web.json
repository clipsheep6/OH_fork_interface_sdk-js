  "attributes": [
    {
      "name": "src",
      "supportedDevices": ["phone", "tablet", "tv"],
      "required": false,
      "description": [
        {
          "name": "Set the URL of the web page to be displayed. The domain name of the web page must be HTTPS and archived by the ICP.",
          "supportedDevices": ["phone", "tablet", "tv"]
        }
      ],
      "type": "string"
    },
    {
      "name": "id",
      "supportedDevices": ["phone", "tablet", "tv"],
      "required": false,
      "description": [
        {
          "name": "Unique ID of a component.",
          "supportedDevices": ["phone", "tablet", "tv"]
        }
      ],
      "type": "string"
    }
  ],
  "events": [
    {
      "name": "pagestart",
      "supportedDevices": ["phone", "tablet", "tv"],
      "description": [
        {
          "name": "Triggered when a web page is loaded",
          "supportedDevices": ["phone", "tablet", "tv"]
        }
      ]
    },
    {
      "name": "pagefinish",
      "supportedDevices": ["phone", "tablet", "tv"],
      "description": [
        {
