        }
      ]
    },
    {
      "name": "message",
      "supportedDevices": ["phone", "tablet", "tv"],
      "version": [
        {
          "name": "v7.0"
        }
      ],
      "description": [
        {
          "name": "Triggered when a message sent from a web page is received.",
          "supportedDevices": ["phone", "tablet", "tv"]
        }
      ]
    }
