        }
      ],
      "type": "string"
    },
    {
      "name": "disabled",
      "supportedDevices": ["phone", "tablet", "tv", "wearable"],
      "required": false,
      "description": [
        {
          "name": "Whether the component is disabled. If the component is disabled, the component cannot respond to user interaction.",
          "supportedDevices": ["phone", "tablet", "tv", "wearable"]
        }
      ],
      "type": "enum",
      "options": [
        {
          "name": "true"
        },
        {
          "name": "false"
        }
      ]
    },
    {
      "name": "focusable",
      "supportedDevices": ["phone", "tablet", "tv", "wearable"],
      "required": false,
      "description": [
        {
          "name": "Whether the component can gain the focus. When focusable is set to true, the component can gain the focus if there is a focusable child component. If this attribute is set to false, the component and its child components cannot gain the focus.",
          "supportedDevices": ["phone", "tablet", "tv", "wearable"]
        }
      ],
      "type": "enum",
      "options": [
        {
          "name": "true"
        },
        {
          "name": "false"
        }
      ]
    },
    {
      "name": "data-*",
